{
	"name": "COVID-19-RECTIFY",
	"version": "1.0",
	"description": "Build an COVID-19 restifier!",
	"permissions": ["activeTab", "declarativeContent"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"icons": {
		"16": "images/virus.png",
		"32": "images/virus.png",
		"48": "images/virus.png",
		"128": "images/virus.png"
	},
	"manifest_version": 2
}
